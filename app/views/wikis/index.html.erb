<h1><u>My Wikis</u></h1>
<% policy_scope(@wikis).each do |wiki| %>
  <h5>
    <%= link_to wiki.title, wiki %> </br>
  </h5>
  <%= link_to "Edit", edit_wiki_path(wiki), class: 'btn btn-success' %>

  <%= link_to  "Delete", [wiki], method: :delete, data: { confirm: 'Are you sure you want to delete this item?'}, class: 'btn btn-primary btn' %>
<% end %>

</br>
</br>
</br>

<%= link_to "New Wiki", new_wiki_path, class: 'btn btn-success' %>
